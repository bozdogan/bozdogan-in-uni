<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bozdogan.control.HostGame">

   <top>
      <HBox BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
         <Button mnemonicParsing="false" onAction="#goBack" text="Go Back">
            <font>
               <Font name="Rockwell" size="18.0" />
            </font>
         </Button>
      </HBox>
   </top>
   <center>
      <Button mnemonicParsing="false" onAction="#start1P" text="Single Player Mode" BorderPane.alignment="TOP_CENTER">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
         <BorderPane.margin>
            <Insets top="50.0" />
         </BorderPane.margin>
      </Button>
   </center>
</BorderPane>
